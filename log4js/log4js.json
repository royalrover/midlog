{
  "customBaseDir" :"/Users/showjoy/github/midlog/log4js/",
  "customDefaultAtt" :{
    "type": "file",
    "absolute": true,
    "alwaysIncludePattern": true
  },
  "appenders": [
    {
      "category": "logInfo",
      "filename": "info.log",
      "layout": {
        "type": "pattern",
        "pattern": "%d %r [%x{name}:%z] %p - %m%n",
        "tokens": {
          "name": "MidProxy"
        }
      }
    },
    {
      "category": "logErr",
      "filename": "error.log",
      "layout": {
        "type": "pattern",
        "pattern": "%d %[%r [%x{name}:%z] %p -%] %m%n",
        "tokens": {
          "name": "MidProxy"
        }
      }
    },
    {
      "category": "trace",
      "filename": "trace.log",
      "layout": {
        "type": "pattern",
        "pattern": "%d %[%r [%x{name}:%z] %p -%] %m%n",
        "tokens": {
          "name": "MidProxy"
        }
      }
    }
  ],
  "replaceConsole": true,
  "levels":{ "trace": "TRACE", "logInfo": "INFO", "logErr": "ERROR"}
}